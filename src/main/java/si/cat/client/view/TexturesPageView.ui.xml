<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui" xmlns:p="urn:import:thothbot.parallax.core.client"
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<b:WellForm ui:field="texturesForm">
			<b:ControlLabel>
				<b:Label type="INFO">Textures</b:Label>
			</b:ControlLabel>
			<b:ListBox multipleSelect="true" ui:field="texturesList"></b:ListBox>
			<b:ButtonToolbar>
				<b:Button ui:field="addTextureHandler" type="SUCCESS" icon="PLUS" />
				<b:Button ui:field="updateTextureHandler" type="INFO" icon="EDIT" />
				<b:Button ui:field="removeTextureHandler" type="DANGER"
					icon="REMOVE" />
			</b:ButtonToolbar>
		</b:WellForm>
	</g:HTMLPanel>
</ui:UiBinder>


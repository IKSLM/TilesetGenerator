<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui" xmlns:p="urn:import:thothbot.parallax.core.client"
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<b:WellForm>
			<b:ButtonToolbar>
				<b:Button ui:field="save" type="SUCCESS" icon="UPLOAD"
					title="Save" />
				<b:Button ui:field="saveAs" type="SUCCESS" icon="UPLOAD_ALT"
					title="Save as" />
				<b:Button ui:field="open" type="SUCCESS" icon="FOLDER_OPEN"
					title="Open" />
				<b:Button ui:field="clear" type="SUCCESS" icon="REMOVE"
					title="Clear" />
			</b:ButtonToolbar>
		</b:WellForm>
	</g:HTMLPanel>
</ui:UiBinder>


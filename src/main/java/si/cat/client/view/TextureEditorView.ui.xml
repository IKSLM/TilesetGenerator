<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:p="urn:import:thothbot.parallax.core.client"
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<g:HTMLPanel>
		<!-- <b:PageHeader></b:PageHeader> -->
		<b:ControlLabel>
			<b:Label type="INFO">Mag filter</b:Label>
		</b:ControlLabel>
		<b:ListBox ui:field="textureMagFilter" width="150px" />
		<b:ControlLabel>
			<b:Label type="INFO">Min filter</b:Label>
		</b:ControlLabel>
		<b:ListBox ui:field="textureMinFilter" width="150px" />
		<b:ButtonToolbar>
			<b:Button ui:field="applyFilters" title="Apply" icon="SAVE"
				type="INVERSE" />
		</b:ButtonToolbar>
		<b:ControlLabel>
			<b:Label type="INFO">Texture preview</b:Label>
		</b:ControlLabel>
		<b:Image ui:field="texturePreview" />
	</g:HTMLPanel>
</ui:UiBinder>

